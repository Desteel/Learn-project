{
  "name": "learnproject",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "install": "yarn --cwd ./server install && yarn --cwd ./client install",
    "server:start": "yarn --cwd ./server start",
    "server:build": "yarn --cwd ./server build",
    "server:run": "yarn --cwd ./server run",
    "client:start": "yarn --cwd ./client start",
    "client:build": "yarn --cwd ./client build",
    "client:test": "yarn --cwd ./client test",
    "client:ttab:start": "ttab yarn client:start",
    "client:ttab:build": "ttab yarn client:build",
    "start": "run-p server:start client:ttab:start",
    "build": "run-p server:build client:ttab:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Desteel/Learn-project.git"
  },
  "author": "Dest",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Desteel/Learn-project/issues"
  },
  "homepage": "https://github.com/Desteel/Learn-project#readme",
  "optionalDependencies": {
    "npm-run-all": "^4.1.5",
    "ttab": "^0.7.0"
  }
}
